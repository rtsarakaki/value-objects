		{
			"key": "MustHaveTheSameStructureThan",
			"messages": [
				{
					"language": "en-US", 
					"message": "${label}  must have the same structure than ${JSON.stringify('model')}." 
				},
				{
					"language": "pt-BR",
					"message": "${label}  deve ter a mesma estrutura que ${JSON.stringify('model')}."
				}
			]
		},
		{
			"key": "validateLabel",
			"messages": [
				{
					"language": "en-US", 
					"message": "Label cannot be empty." 
				},
				{
					"language": "pt-BR",
					"message": "Label não pode ser vazio."
				}
			]
		},
		{
			"key": "enumError",
			"messages": [
				{
					"language": "en-US", 
					"message": "Enum is not valid." 
				},
				{
					"language": "pt-BR",
					"message": "Enum não é valida."
				}
			]
		},
		{
			"key": "CannotContainNumbers",
			"messages": [
				{
					"language": "en-US", 
					"message": "${label}  cannot contain numbers." 
				},
				{
					"language": "pt-BR",
					"message": "${label} não pode conter números."
				}
			]
		},
		{
			"key": "replaceTagsInMessage",
			"messages": [
				{
					"language": "en-US", 
					"message": "Invalid tag list content." 
				},
				{
					"language": "pt-BR",
					"message": "Lista de tags é inválida."
				}
			]
		},
		{
			"key": "validateRegex",
			"messages": [
				{
					"language": "en-US", 
					"message": "Regex '${regex}' is invalid." 
				},
				{
					"language": "pt-BR",
					"message": "A expressão regular ${regex} não é válida."
				}
			]
		},
		{
			"key": "RegexMatch",
			"messages": [
				{
					"language": "en-US", 
					"message": "${label} ${regex}." 
				},
				{
					"language": "pt-BR",
					"message": "${label} ${regex}."
				}
			]
		},
		{
			"key": "MustStartWithAlphaNumeric",
			"messages": [
				{
					"language": "en-US", 
					"message": "${label} must start with a letter or number." 
				},
				{
					"language": "pt-BR",
					"message": "${label} deve iniciar com letra ou número."
				}
			]
		},
		{
			"key": "MustEndWithAlphaNumeric",
			"messages": [
				{
					"language": "en-US", 
					"message": "${label} must end with a letter or number." 
				},
				{
					"language": "pt-BR",
					"message": "${label} deve terminar com letra ou número."
				}
			]
		},
		{
			"key": "IsValidDate",
			"messages": [
				{
					"language": "en-US", 
					"message": "${label} ${value} is not a valid date." 
				},
				{
					"language": "pt-BR",
					"message": "${label} ${value} não é uma data valida."
				}
			]
		}
	]
}